@model AssessmentFrontEnd.Models.DocumentVM
@{
    ViewData["Title"] = "Document information";
}




<br />
<br />
<div class="container" id="DocDetails">
    <!-- Inputs -->
    <h3>Document information</h3>
    <hr>

    <div class="row mt-5">
        <div class="col-md-12">
            <div class="form-group">
                <label asp-for="Name"></label>
                <input type="text" class="form-control" disabled>
                <br />
            </div>
            <div class="form-group">
                <label>Created date</label>
                <input type="text" class="form-control" disabled>
                <br />
            </div>
            <div class="form-group">
                <label asp-for="Date"></label>
                <input type="text"  class="form-control" disabled>
                <br />
            </div>

            <div class="form-group">
                <label asp-for="Priority"></label>
                <input type="text"  class="form-control" disabled>

                <br />

            </div>
            <div class="form-group">
                <label>Due Date</label>
                <input type="text"  class="form-control" disabled>
                <br />
            </div>
            <br />
            <div class="form-group">
                <a class=" btn btn-primary" href="/Document/update/"  style="float:right">Upload a Document</a>
            </div>

        </div>

    </div> <!-- end of inputs -->

</div>
@section _ValidationScript
    {
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
 }